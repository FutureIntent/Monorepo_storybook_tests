{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@theme": [
        "theme/"
      ],
      "@theme/*": [
        "theme/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@store": [
        "src/store"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@pages": [
        "pages"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@providers": [
        "src/providers"
      ],
      "@providers/*": [
        "src/providers/*"
      ],
      "@atoms": [
        "src/atoms"
      ],
      "@atoms/*": [
        "src/atoms/*"
      ],
      "@molecules": [
        "src/molecules"
      ],
      "@molecules/*": [
        "src/molecules/*"
      ],
      "@organisms": [
        "src/organisms"
      ],
      "@organisms/*": [
        "src/organisms/*"
      ],
      "@templates": [
        "src/templates"
      ],
      "@templates/*": [
        "src/templates/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@hooks": [
        "src/hooks"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@public/*": [
        "public/*"
      ]
    },
    "typeRoots" : ["node_modules/@types", "types"]
  },
  "include": [
    "next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": [
    "cryo-app-therapies/node_modules"
  ]
}
