    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.15",
    "@types/styled-system__should-forward-prop": "^5.1.2",
    "@types/swiper": "^6.0.0",
    "@types/testing-library__jest-dom": "^5.14.5",
