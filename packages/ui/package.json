{
  "name": "cryo-ui-kit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "start-storybook --quiet -p 6006",
    "lint": "eslint *.ts*",
    "build": "build-storybook -- -o public",
    "svgr:monochrome": "npx @svgr/cli --jsx-runtime=\"automatic\" --ref --icon --out-dir icons/svg --no-svgo --memo --replace-attr-values \"#000=currentColor\" --typescript --index-template utils/svgrTemplate.js assets/svg",
    "svgr:colored": "npx @svgr/cli --jsx-runtime=\"automatic\" --ref --icon --out-dir icons/svgColored --no-svgo --memo --typescript --index-template utils/svgrTemplate.js assets/svgColored",
    "test": "jest"
  },
  "dependencies": {
