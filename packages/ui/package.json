    "@storybook/react": "^6.5.12",
    "@styled-system/should-forward-prop": "^5.1.5",
    "country-flag-icons": "^1.5.5",
    "jest-environment-jsdom": "^29.2.2",
    "nanoid": "^4.0.0",
